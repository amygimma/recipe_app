<h1>New Post</h1>

<%= form_for :post, url: post_create_path do |f| %>
  <p>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </p>
  <p>  
    <%= f.label :author %>
    <%= f.text_field :author %>
  </p>
  
  <p>  
    <%= f.label :body %>
    <%= f.text_area :body %>
  </p>
  
  <p>
    <%= f.submit %>
  </p>
  
<% end %>
